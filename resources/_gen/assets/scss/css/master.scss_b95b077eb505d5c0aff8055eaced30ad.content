rset=UTF-8,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 598 592' preserveAspectRatio='none'><polygon points='598 592 598 0 0 420 598 592' fill='%23fdbe0b'/></svg>") 100% 92%/84% 50% no-repeat}@media(min-width:540px){#mixer{padding:120px 0 150px;text-align:right;background-size:41% 94%;background-position:100% 50%}}#mixer h2{color:#fdbe0b}@media(min-width:540px){#mixer h2{grid-column:2/6}}@media(min-width:540px){#mixer p{grid-column:2/6}}#mixer a{background:#fdbe0b}@media(min-width:540px){#mixer a{grid-column:2/6;justify-self:end}}@media(min-width:540px){#mixer .img{grid-column:7/12;grid-row:1/4}}#mixer .img img{-o-object-position:100% 0;object-position:100% 0}#action{padding:90px 0}@media(min-width:540px){#action{padding:180px 0}}#action h2{font-size:1.8rem;font-weight:700;color:#8338ec;text-align:center}#action figure{display:table;padding:14px 20px 18px;margin:30px auto 0;font:.9rem Menlo,Monaco,monospace;line-height:1.5;border-radius:6px;background:#f4f4f4;color:#8338ec}@media(min-width:540px){#action figure{padding:26px 32px 30px;font-size:1rem}}#action figure span{color:#848484}#action p{margin:30px;font-weight:700;color:#8338ec;text-align:center}#action a{margin:30px auto 0;color:#8338ec;background:#48fff1}